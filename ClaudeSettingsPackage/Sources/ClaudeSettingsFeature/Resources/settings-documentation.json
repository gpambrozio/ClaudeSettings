{
  "version": "2025-11",
  "categories": [
    {
      "id": "model-behavior",
      "name": "Model & Behavior",
      "description": "Configure Claude's model selection and response behavior",
      "settings": [
        {
          "key": "model",
          "type": "string",
          "description": "Override the default model used by Claude Code.",
          "examples": [
            {
              "code": "{\n  \"model\": \"claude-sonnet-4-5-20250929\"\n}",
              "description": "Set specific model version",
              "id": "32c2fe51-e649-49e3-b985-bbae152b142c"
            }
          ]
        },
        {
          "key": "outputStyle",
          "type": "string",
          "minLength": 1,
          "description": "Controls the output style for assistant responses. Can be 'default', 'Explanatory', 'Learning', or a custom style name.",
          "examples": [
            {
              "code": "{\n  \"outputStyle\": \"Explanatory\"\n}",
              "description": "Use explanatory output style",
              "id": "a1b2c3d4-e5f6-7890-abcd-ef1234567890"
            },
            {
              "code": "{\n  \"outputStyle\": \"Learning\"\n}",
              "description": "Use learning-focused output style",
              "id": "b2c3d4e5-f6a7-8901-bcde-f12345678901"
            }
          ]
        },
        {
          "key": "statusLine",
          "type": "object",
          "description": "Custom status line display configuration. Requires 'type' (must be 'command') and 'command' (shell command to execute).",
          "required": ["type", "command"],
          "properties": {
            "type": {
              "type": "string",
              "const": "command"
            },
            "command": {
              "type": "string",
              "minLength": 1
            },
            "padding": {
              "type": "number"
            }
          },
          "examples": [
            {
              "code": "{\n  \"statusLine\": {\n    \"type\": \"command\",\n    \"command\": \"~/.claude/statusline.sh\"\n  }\n}",
              "description": "Custom status line using shell script",
              "id": "4af77421-8c04-4d66-af84-d44c709fd0fb"
            }
          ]
        },
        {
          "key": "forceLoginMethod",
          "type": "string",
          "enumValues": [
            "claudeai",
            "console"
          ],
          "description": "Force a specific login method: 'claudeai' for Claude Pro/Max, 'console' for Console billing.",
          "examples": [
            {
              "code": "{\n  \"forceLoginMethod\": \"console\"\n}",
              "description": "Force console login method",
              "id": "c2eef542-7e9b-4176-a269-f4d945eb7dde"
            }
          ]
        },
        {
          "key": "alwaysThinkingEnabled",
          "type": "boolean",
          "description": "Whether extended thinking is always enabled (default: false).",
          "examples": [
            {
              "code": "{\n  \"alwaysThinkingEnabled\": true\n}",
              "description": "Enable extended thinking by default",
              "id": "f1a2b3c4-d5e6-7890-abcd-ef1234567890"
            }
          ]
        },
        {
          "key": "spinnerTipsEnabled",
          "type": "boolean",
          "description": "Whether to show tips in the spinner.",
          "examples": [
            {
              "code": "{\n  \"spinnerTipsEnabled\": false\n}",
              "description": "Disable spinner tips",
              "id": "a3b4c5d6-e7f8-9012-bcde-f12345678901"
            }
          ]
        }
      ]
    },
    {
      "id": "authentication",
      "name": "Authentication",
      "description": "Control authentication methods and organization selection",
      "settings": [
        {
          "key": "apiKeyHelper",
          "type": "string",
          "minLength": 1,
          "description": "Path to a script that outputs authentication values.",
          "relatedEnvVars": [
            "CLAUDE_CODE_API_KEY_HELPER_TTL_MS"
          ],
          "examples": [
            {
              "code": "{\n  \"apiKeyHelper\": \"/bin/generate_temp_api_key.sh\"\n}",
              "description": "Use custom script for authentication",
              "id": "0de23147-16e5-467a-896a-8e82e36368f4"
            }
          ]
        },
        {
          "key": "forceLoginOrgUUID",
          "type": "string",
          "minLength": 1,
          "description": "Organization UUID to use for OAuth login.",
          "examples": [
            {
              "code": "{\n  \"forceLoginOrgUUID\": \"xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx\"\n}",
              "description": "Auto-select organization",
              "id": "ae9a0a99-edfc-4f76-82de-7efdb794b1da"
            }
          ]
        },
        {
          "key": "awsAuthRefresh",
          "type": "string",
          "minLength": 1,
          "description": "Path to a script that refreshes AWS authentication.",
          "examples": [
            {
              "code": "{\n  \"awsAuthRefresh\": \"aws sso login --profile myprofile\"\n}",
              "description": "Refresh AWS SSO credentials",
              "id": "d1e2f3a4-b5c6-7890-abcd-ef1234567890"
            }
          ]
        },
        {
          "key": "awsCredentialExport",
          "type": "string",
          "minLength": 1,
          "description": "Path to a script that exports AWS credentials.",
          "examples": [
            {
              "code": "{\n  \"awsCredentialExport\": \"/bin/generate_aws_grant.sh\"\n}",
              "description": "Export AWS credentials via script",
              "id": "e2f3a4b5-c6d7-8901-bcde-f12345678901"
            }
          ]
        },
        {
          "key": "otelHeadersHelper",
          "type": "string",
          "minLength": 1,
          "description": "Path to a script that outputs OpenTelemetry headers.",
          "examples": [
            {
              "code": "{\n  \"otelHeadersHelper\": \"/bin/otel_headers.sh\"\n}",
              "description": "Custom OpenTelemetry headers script",
              "id": "f3a4b5c6-d7e8-9012-cdef-123456789012"
            }
          ]
        }
      ]
    },
    {
      "id": "workspace",
      "name": "Workspace Management",
      "description": "Configure workspace behavior, cleanup, and environment",
      "settings": [
        {
          "key": "cleanupPeriodDays",
          "type": "integer",
          "defaultValue": "30",
          "minimum": 0,
          "description": "Number of days to retain chat transcripts (0 to disable cleanup).",
          "examples": [
            {
              "code": "{\n  \"cleanupPeriodDays\": 60\n}",
              "description": "Keep transcripts for 60 days",
              "id": "07343986-7681-4f0c-b70e-a5d698122bec"
            }
          ]
        },
        {
          "key": "includeCoAuthoredBy",
          "type": "boolean",
          "defaultValue": "true",
          "description": "Whether to include Claude's co-authored by attribution in commits and PRs (defaults to true).",
          "examples": [
            {
              "code": "{\n  \"includeCoAuthoredBy\": false\n}",
              "description": "Disable co-author byline",
              "id": "c96eb2ac-c863-45d8-9e98-849e0429734f"
            }
          ]
        },
        {
          "key": "env",
          "type": "object",
          "defaultValue": "{}",
          "description": "Environment variables to set for Claude Code sessions. Variable names must match pattern ^[A-Z_][A-Z0-9_]*$.",
          "examples": [
            {
              "code": "{\n  \"env\": {\n    \"ANTHROPIC_MODEL\": \"claude-opus-4-1\",\n    \"ANTHROPIC_SMALL_FAST_MODEL\": \"claude-3-5-haiku-latest\"\n  }\n}",
              "description": "Configure Claude model selection",
              "id": "341a71b9-fe48-4525-891c-900700b9d8cb"
            }
          ]
        },
        {
          "key": "companyAnnouncements",
          "type": "array",
          "itemType": "string",
          "itemMinLength": 1,
          "description": "Company announcements to display at startup (one will be randomly selected if multiple are provided).",
          "examples": [
            {
              "code": "{\n  \"companyAnnouncements\": [\n    \"Welcome to Acme Corp development environment!\",\n    \"Remember to run tests before committing!\",\n    \"Check the wiki for coding standards.\"\n  ]\n}",
              "description": "Configure startup messages",
              "id": "5224bb73-6285-4d17-8f3b-7e3c03a9207b"
            }
          ]
        },
        {
          "key": "skipWebFetchPreflight",
          "type": "boolean",
          "description": "Skip the WebFetch blocklist check for enterprise environments with restrictive security policies.",
          "examples": [
            {
              "code": "{\n  \"skipWebFetchPreflight\": true\n}",
              "description": "Skip WebFetch blocklist check",
              "id": "d58fa573-a4fa-454c-a8cc-a38bb4ab6622"
            }
          ]
        }
      ]
    },
    {
      "id": "tool-integration",
      "name": "Tool Integration",
      "description": "Configure hooks and tool execution behavior",
      "settings": [
        {
          "key": "hooks",
          "type": "object",
          "description": "Hooks configuration for executing commands at specific points in Claude Code's lifecycle.",
          "hookTypes": [
            "PreToolUse",
            "PostToolUse",
            "Notification",
            "UserPromptSubmit",
            "Stop",
            "SubagentStop",
            "PreCompact",
            "SessionStart",
            "SessionEnd"
          ],
          "examples": [
            {
              "code": "{\n  \"hooks\": {\n    \"PostToolUse\": [\n      {\n        \"matcher\": \"Edit|Write\",\n        \"hooks\": [\n          {\n            \"type\": \"command\",\n            \"command\": \"prettier --write\",\n            \"timeout\": 5\n          }\n        ]\n      }\n    ]\n  }\n}",
              "description": "Run prettier after file edits",
              "id": "2297c4c7-08b3-41ea-8d50-2e663adbb7b8"
            }
          ]
        },
        {
          "key": "disableAllHooks",
          "type": "boolean",
          "defaultValue": "false",
          "description": "Disable all hooks and statusLine execution.",
          "examples": [
            {
              "code": "{\n  \"disableAllHooks\": true\n}",
              "description": "Disable all hooks",
              "id": "3500016b-cbd4-46ec-8a49-8e868778b5fb"
            }
          ]
        }
      ]
    },
    {
      "id": "permissions",
      "name": "Permission System",
      "description": "Control tool access and file permissions",
      "settings": [
        {
          "key": "permissions.allow",
          "type": "array",
          "itemType": "string",
          "uniqueItems": true,
          "description": "List of permission rules for allowed operations.",
          "patterns": [
            "Bash(command:*) - Prefix matching for bash commands",
            "Read(glob/pattern/**) - Glob patterns for file operations",
            "WebFetch(domain:example.com) - Domain restrictions"
          ],
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"allow\": [\n      \"Bash(git commit:*)\",\n      \"Bash(npm run lint:*)\",\n      \"Edit(/src/**/*.ts)\",\n      \"WebFetch(domain:github.com)\"\n    ]\n  }\n}",
              "description": "Allow common development tools",
              "id": "764e445b-392e-4473-b101-66c6ec91fa08"
            }
          ]
        },
        {
          "key": "permissions.ask",
          "type": "array",
          "itemType": "string",
          "uniqueItems": true,
          "description": "List of permission rules that should always prompt for confirmation.",
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"ask\": [\n      \"Bash(gh pr create:*)\",\n      \"Bash(git commit:*)\"\n    ]\n  }\n}",
              "description": "Require confirmation for git operations",
              "id": "07e56fa7-0732-4ca7-a789-8af8f753116d"
            }
          ]
        },
        {
          "key": "permissions.deny",
          "type": "array",
          "itemType": "string",
          "uniqueItems": true,
          "description": "List of permission rules for denied operations.",
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"deny\": [\n      \"Read(*.env)\",\n      \"Bash(rm:*)\",\n      \"Bash(curl:*)\"\n    ]\n  }\n}",
              "description": "Block access to sensitive files and commands",
              "id": "5e7d1236-d35d-4d97-b547-0f9357f6857f"
            }
          ]
        },
        {
          "key": "permissions.additionalDirectories",
          "type": "array",
          "itemType": "string",
          "itemMinLength": 1,
          "uniqueItems": true,
          "description": "Additional directories to include in the permission scope.",
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"additionalDirectories\": [\n      \"//Users/alice/Documents\",\n      \"~/projects\"\n    ]\n  }\n}",
              "description": "Add access to additional directories",
              "id": "102ea426-efb4-48e5-9ee3-df91689632d0"
            }
          ]
        },
        {
          "key": "permissions.defaultMode",
          "type": "string",
          "enumValues": [
            "acceptEdits",
            "bypassPermissions",
            "default",
            "plan"
          ],
          "description": "Default permission mode when Claude Code needs access.",
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"defaultMode\": \"acceptEdits\"\n  }\n}",
              "description": "Start in accept edits mode",
              "id": "9114055a-772e-4937-ae8a-091b8005c132"
            }
          ]
        },
        {
          "key": "permissions.disableBypassPermissionsMode",
          "type": "string",
          "enumValues": [
            "disable"
          ],
          "description": "Disable the ability to bypass permission prompts.",
          "examples": [
            {
              "code": "{\n  \"permissions\": {\n    \"disableBypassPermissionsMode\": \"disable\"\n  }\n}",
              "description": "Disable permission bypass",
              "id": "9884edfd-1321-46ba-9c22-9f30fbadd784"
            }
          ]
        }
      ]
    },
    {
      "id": "sandbox",
      "name": "Sandbox Configuration",
      "description": "Configure command sandboxing for filesystem and network isolation",
      "platformNote": "Only available on macOS and Linux",
      "settings": [
        {
          "key": "sandbox.enabled",
          "type": "boolean",
          "defaultValue": "false",
          "description": "Enable sandboxed bash.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"enabled\": true\n  }\n}",
              "description": "Enable sandbox",
              "id": "9dca8d88-9804-44d5-a129-270026e1b8a1"
            }
          ]
        },
        {
          "key": "sandbox.autoAllowBashIfSandboxed",
          "type": "boolean",
          "description": "Automatically allow bash commands without prompting when they run in the sandbox. Only applies to commands that will run sandboxed.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"autoAllowBashIfSandboxed\": true\n  }\n}",
              "description": "Auto-allow sandboxed bash commands",
              "id": "53677f3b-1d54-4d62-8717-3de05c2cd754"
            }
          ]
        },
        {
          "key": "sandbox.allowUnsandboxedCommands",
          "type": "boolean",
          "defaultValue": "true",
          "description": "Allow commands to run outside the sandbox via the dangerouslyDisableSandbox parameter. When false, the dangerouslyDisableSandbox parameter is completely ignored and all commands must run sandboxed.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"allowUnsandboxedCommands\": false\n  }\n}",
              "description": "Enforce strict sandboxing",
              "id": "b12f20ca-20d0-421a-b818-8b5e09b03fcc"
            }
          ]
        },
        {
          "key": "sandbox.excludedCommands",
          "type": "array",
          "itemType": "string",
          "description": "Commands that should never run in the sandbox (e.g., [\"git\", \"docker\"]).",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"excludedCommands\": [\n      \"git\",\n      \"docker\",\n      \"npm\"\n    ]\n  }\n}",
              "description": "Exclude commands from sandbox",
              "id": "9e1ba135-f6df-435d-8855-1a9747766a73"
            }
          ]
        },
        {
          "key": "sandbox.ignoreViolations",
          "type": "object",
          "description": "Map of command patterns to filesystem paths to ignore sandbox violations for. Use \"*\" to match all commands.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"ignoreViolations\": {\n      \"npm\": [\"/Users/*/npm-cache\"],\n      \"*\": [\"/tmp/allowed\"]\n    }\n  }\n}",
              "description": "Ignore specific sandbox violations",
              "id": "e7c900b1-f23b-428b-85b0-7db474e4028f"
            }
          ]
        },
        {
          "key": "sandbox.network.allowUnixSockets",
          "type": "array",
          "itemType": "string",
          "description": "Allow Unix domain sockets for local IPC (SSH agent, Docker, etc.). Provide an array of specific paths. Defaults to blocking if not specified.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"network\": {\n      \"allowUnixSockets\": [\n        \"/var/run/docker.sock\",\n        \"/tmp/my-app.sock\"\n      ]\n    }\n  }\n}",
              "description": "Allow specific Unix sockets",
              "id": "c557bf8f-a2cf-4fad-8fe2-ab821e3b9b25"
            }
          ]
        },
        {
          "key": "sandbox.network.allowLocalBinding",
          "type": "boolean",
          "defaultValue": "false",
          "platformNote": "macOS only",
          "description": "Allow binding to local network addresses (e.g., localhost ports). Defaults to false if not specified.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"network\": {\n      \"allowLocalBinding\": true\n    }\n  }\n}",
              "description": "Allow localhost port binding",
              "id": "ee7fa086-01ce-4bc8-892f-0519d7908996"
            }
          ]
        },
        {
          "key": "sandbox.network.httpProxyPort",
          "type": "integer",
          "minimum": 1,
          "maximum": 65535,
          "description": "HTTP proxy port to use for network filtering. If not specified, a proxy server will be started automatically.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"network\": {\n      \"httpProxyPort\": 8080\n    }\n  }\n}",
              "description": "Set custom HTTP proxy port",
              "id": "96eb9b80-4e90-4f9d-9a18-d27e0a4b1a6c"
            }
          ]
        },
        {
          "key": "sandbox.network.socksProxyPort",
          "type": "integer",
          "minimum": 1,
          "maximum": 65535,
          "description": "SOCKS proxy port to use for network filtering. If not specified, a proxy server will be started automatically.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"network\": {\n      \"socksProxyPort\": 1080\n    }\n  }\n}",
              "description": "Set custom SOCKS proxy port",
              "id": "11cfbcd4-4a4c-4579-bd73-daf6022ad206"
            }
          ]
        },
        {
          "key": "sandbox.enableWeakerNestedSandbox",
          "type": "boolean",
          "defaultValue": "false",
          "description": "Enable weaker sandbox mode for unprivileged docker environments where --proc mounting fails. This significantly reduces the strength of the sandbox and should only be used when this risk is acceptable.",
          "examples": [
            {
              "code": "{\n  \"sandbox\": {\n    \"enableWeakerNestedSandbox\": true\n  }\n}",
              "description": "Enable weaker nested sandbox",
              "id": "a5329e1a-28e3-4093-be7b-ca144ca2b49a"
            }
          ]
        }
      ]
    },
    {
      "id": "mcp",
      "name": "MCP Server Configuration",
      "description": "Configure Model Context Protocol servers",
      "settings": [
        {
          "key": "enableAllProjectMcpServers",
          "type": "boolean",
          "defaultValue": "false",
          "description": "Whether to automatically approve all MCP servers in the project.",
          "examples": [
            {
              "code": "{\n  \"enableAllProjectMcpServers\": true\n}",
              "description": "Auto-approve project MCP servers",
              "id": "664d8b45-3719-4e0a-b9c2-66a2dbf5df79"
            }
          ]
        },
        {
          "key": "enabledMcpjsonServers",
          "type": "array",
          "itemType": "string",
          "itemMinLength": 1,
          "description": "List of approved MCP servers from .mcp.json.",
          "examples": [
            {
              "code": "{\n  \"enabledMcpjsonServers\": [\n    \"memory\",\n    \"github\"\n  ]\n}",
              "description": "Enable specific MCP servers",
              "id": "095610b2-6f66-478c-aded-df817f40ca3a"
            }
          ]
        },
        {
          "key": "disabledMcpjsonServers",
          "type": "array",
          "itemType": "string",
          "itemMinLength": 1,
          "description": "List of rejected MCP servers from .mcp.json.",
          "examples": [
            {
              "code": "{\n  \"disabledMcpjsonServers\": [\n    \"filesystem\"\n  ]\n}",
              "description": "Disable specific MCP servers",
              "id": "bc3c379f-f125-4c79-8483-02fc29f6966f"
            }
          ]
        },
        {
          "key": "allowedMcpServers",
          "type": "array",
          "itemType": "object",
          "description": "Enterprise allowlist of MCP servers that can be used. Applies to all scopes including enterprise servers from managed-mcp.json. If undefined, all servers are allowed. If empty array, no servers are allowed. Denylist takes precedence - if a server is on both lists, it is denied.",
          "examples": [
            {
              "code": "{\n  \"allowedMcpServers\": [\n    { \"serverName\": \"github\" },\n    { \"serverName\": \"memory\" }\n  ]\n}",
              "description": "Restrict to approved servers only",
              "id": "5a68d8c0-d202-4837-b783-3a536d9794e4"
            }
          ]
        },
        {
          "key": "deniedMcpServers",
          "type": "array",
          "itemType": "object",
          "description": "Enterprise denylist of MCP servers that are explicitly blocked. If a server is on the denylist, it will be blocked across all scopes including enterprise. Denylist takes precedence over allowlist - if a server is on both lists, it is denied.",
          "examples": [
            {
              "code": "{\n  \"deniedMcpServers\": [\n    { \"serverName\": \"filesystem\" },\n    { \"serverName\": \"experimental\" }\n  ]\n}",
              "description": "Block specific MCP servers",
              "id": "dfcbf2ec-d9ea-4cec-a871-6be32a831fba"
            }
          ]
        }
      ]
    },
    {
      "id": "plugins",
      "name": "Plugin System",
      "description": "Configure plugin enablement and marketplaces",
      "settings": [
        {
          "key": "enabledPlugins",
          "type": "object",
          "description": "Enabled plugins using plugin-id@marketplace-id format. Values can be boolean (true/false) or array of strings for extended configuration.",
          "examples": [
            {
              "code": "{\n  \"enabledPlugins\": {\n    \"formatter@anthropic-tools\": true,\n    \"custom-linter@company-marketplace\": false\n  }\n}",
              "description": "Enable or disable specific plugins",
              "id": "88e62e71-4ecb-4896-a530-1ffbb6b3d659"
            }
          ]
        },
        {
          "key": "extraKnownMarketplaces",
          "type": "object",
          "description": "Additional marketplaces to make available for this repository. Typically used in repository .claude/settings.json to ensure team members have required plugin sources. Keys are marketplace names, values define the source configuration.",
          "examples": [
            {
              "code": "{\n  \"extraKnownMarketplaces\": {\n    \"company-tools\": {\n      \"source\": {\n        \"source\": \"github\",\n        \"repo\": \"company/claude-plugins\",\n        \"ref\": \"main\"\n      }\n    },\n    \"local-dev\": {\n      \"source\": {\n        \"source\": \"directory\",\n        \"path\": \"/Users/dev/my-plugins\"\n      }\n    }\n  }\n}",
              "description": "Add custom plugin marketplaces",
              "id": "e265edc0-3d25-4ecc-be5a-f4fea1c16b67"
            }
          ]
        },
        {
          "key": "skippedMarketplaces",
          "type": "array",
          "itemType": "string",
          "itemMinLength": 1,
          "description": "List of marketplace names the user has chosen not to install when prompted.",
          "examples": [
            {
              "code": "{\n  \"skippedMarketplaces\": [\n    \"experimental-tools\",\n    \"beta-marketplace\"\n  ]\n}",
              "description": "Skip specific marketplaces",
              "id": "59762550-4bec-4e10-9c2c-67437b5fa529"
            }
          ]
        },
        {
